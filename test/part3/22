var a =	10; 
var b = 20; 

function bmethod(y) {
  var b = 30;
 
  function dmethod(z) {
    var a = z;
    return a + cmethod(z);
  }
  return a + b + dmethod(y);
}

function cmethod(x) {
  var a = 40;
  if (x==0)
    return a + b;
  else
    return bmethod(x-1) + a + b;
}

function main() {
  var b = 2;
  return cmethod(b) + a + b;
}